{
  "expo": {
    "name": "NoExcusasFTC",
    "slug": "NoExcusasFTC",
    "scheme": "com.ccraig09.noexcusasftc",
    "version": "3.0.0",
    "orientation": "portrait",
    "icon": "./assets/appOldLogo.png",
    "facebookAutoInitEnabled": true,
    "facebookScheme": "fb287994216182182",
    "facebookAppId": "287994216182182",
    "facebookDisplayName": "NoExcusasFTC",
    "splash": {
      "image": "./assets/icon-noexlogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "notification": {
      "icon": "./assets/logonoe96.png",
      "color": "#000000"
    },
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos to upload user profile images and progress images.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos of results.",
          "isAccessMediaLocationEnabled": true
